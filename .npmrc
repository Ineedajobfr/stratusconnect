# NPM Configuration for StratusConnect
# This file helps prevent common npm issues

# Security settings
audit-level=moderate
fund=false
update-notifier=false

# Performance settings
prefer-offline=true

# Registry settings
registry=https://registry.npmjs.org/

# Install settings
save-exact=false
package-lock=true

# Logging
loglevel=warn
progress=true

# Engine settings
engine-strict=false
